{
  "compilerOptions": {
      "esModuleInterop": true,
      "outDir": "../docs/",
      "module": "esnext",
      "jsx": "react",
      "target": "es5",
      "moduleResolution": "node",
      "declaration": false,
      "noImplicitAny": true,
      "removeComments": true,
      "baseUrl": "./src",
      "types": ["node", "@emotion/core"],
      "paths": {
        "@root": ["../../src"],
        "@types": ["@types"],
        "@constants": ["constants"],
        "@components/*": ["components/*"],
        "@utilities/*": ["utilities/*"]
      }
  },
  "include": ["src/**/*"],
  "exclude": ["node_modules"]
}
  