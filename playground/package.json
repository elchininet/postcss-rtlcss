{
  "name": "postcss-rtlcss-playground",
  "version": "1.0.0",
  "description": "PostCSS plugin to build Cascading Style Sheets (CSS) with Left-To-Right (LTR) and Right-To-Left (RTL) rules",
  "keywords": [
    "postcss",
    "css",
    "postcss-plugin",
    "rtl",
    "ltr",
    "right-to-left",
    "left-to-right",
    "postcss-rtlcss",
    "rtlcss"
  ],
  "scripts": {
    "generate-types": "typed-scss-modules src --aliasPrefixes.@constants src/constants",
    "generate-types:dev": "typed-scss-modules src --aliasPrefixes.@constants src/constants --watch",
    "start": "yarn generate-types:dev & webpack serve --open --config webpack.dev-config.js",
    "build": "yarn generate-types && webpack --config webpack.config.js"
  },
  "author": "ElChiniNet",
  "license": "Apache-2.0",
  "dependencies": {
    "monaco-editor": "^0.36.1",
    "postcss": "8.4.6",
    "postcss-rtlcss": "^4.0.3",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-github-btn": "^1.4.0"
  },
  "devDependencies": {
    "@babel/core": "^7.21.0",
    "@babel/preset-env": "^7.20.2",
    "@babel/preset-react": "^7.18.6",
    "@babel/preset-typescript": "^7.21.0",
    "@types/eslint": "^8.21.1",
    "@types/node": "^18.14.6",
    "@types/react": "^18.0.28",
    "@types/react-dom": "^18.0.11",
    "@typescript-eslint/eslint-plugin": "^5.54.0",
    "@typescript-eslint/parser": "^5.54.0",
    "autoprefixer": "^10.4.13",
    "babel-loader": "^9.1.2",
    "classnames": "^2.3.2",
    "clean-webpack-plugin": "^4.0.0",
    "copy-webpack-plugin": "^11.0.0",
    "css-loader": "^6.7.3",
    "eslint": "^8.35.0",
    "eslint-plugin-react": "^7.32.2",
    "eslint-utils": "^3.0.0",
    "file-loader": "^6.2.0",
    "html-webpack-plugin": "^5.5.0",
    "mini-css-extract-plugin": "^2.7.2",
    "monaco-editor-webpack-plugin": "^7.0.1",
    "node-sass": "^8.0.0",
    "postcss-loader": "^7.0.2",
    "sass-loader": "^13.2.0",
    "typed-scss-modules": "^7.0.2",
    "typescript": "^4.9.5",
    "webpack": "^5.76.0",
    "webpack-cli": "^5.0.1",
    "webpack-dev-server": "^4.11.1"
  },
  "browserslist": [
    "defaults"
  ],
  "engines": {
    "node": ">=12.22.0"
  }
}
