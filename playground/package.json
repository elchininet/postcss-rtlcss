{
  "name": "postcss-rtlcss-playground",
  "version": "1.0.0",
  "description": "PostCSS plugin to build Cascading Style Sheets (CSS) with Left-To-Right (LTR) and Right-To-Left (RTL) rules",
  "keywords": [
    "postcss",
    "css",
    "postcss-plugin",
    "rtl",
    "ltr",
    "right-to-left",
    "left-to-right",
    "postcss-rtlcss",
    "rtlcss"
  ],
  "scripts": {
    "generate-types": "typed-scss-modules src --aliasPrefixes.@constants src/constants",
    "generate-types:dev": "typed-scss-modules src --aliasPrefixes.@constants src/constants --watch",
    "start": "yarn generate-types:dev & webpack serve --open --config webpack.dev-config.js",
    "build": "yarn generate-types && webpack --config webpack.config.js"
  },
  "author": "ElChiniNet",
  "license": "Apache-2.0",
  "dependencies": {
    "monaco-editor": "^0.44.0",
    "postcss": "^8.4.31",
    "postcss-rtlcss": "^4.0.9",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-github-btn": "^1.4.0"
  },
  "devDependencies": {
    "@babel/core": "^7.23.3",
    "@babel/preset-env": "^7.23.3",
    "@babel/preset-react": "^7.23.3",
    "@babel/preset-typescript": "^7.23.3",
    "@types/eslint": "^8.44.7",
    "@types/node": "^20.9.0",
    "@types/react": "^18.2.37",
    "@types/react-dom": "^18.2.15",
    "@typescript-eslint/eslint-plugin": "^6.10.0",
    "@typescript-eslint/parser": "^6.10.0",
    "autoprefixer": "^10.4.16",
    "babel-loader": "^9.1.3",
    "classnames": "^2.3.2",
    "clean-webpack-plugin": "^4.0.0",
    "copy-webpack-plugin": "^11.0.0",
    "css-loader": "^6.8.1",
    "eslint": "^8.53.0",
    "eslint-plugin-react": "^7.33.2",
    "eslint-utils": "^3.0.0",
    "file-loader": "^6.2.0",
    "html-webpack-plugin": "^5.5.3",
    "mini-css-extract-plugin": "^2.7.6",
    "monaco-editor-webpack-plugin": "^7.1.0",
    "node-sass": "^9.0.0",
    "postcss-loader": "^7.3.3",
    "sass-loader": "^13.3.2",
    "typed-scss-modules": "^7.1.4",
    "typescript": "^5.2.2",
    "webpack": "^5.89.0",
    "webpack-cli": "^5.1.4",
    "webpack-dev-server": "^4.15.1"
  },
  "browserslist": [
    "defaults"
  ],
  "engines": {
    "node": ">=18.0.0"
  }
}
